<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kpt">


    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KPT">

        <activity
            android:name=".Feedback"
            android:label="@string/title_activity_feedback"
            android:theme="@style/Theme.KPT.NoActionBar"/>

        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.KPT.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".DivisionActivity"
            android:label="DivisionActivity"
            android:theme="@style/Theme.KPT.NoActionBar"/>

        <activity
            android:name=".FinalGuest"
            android:label="Thank You Guest"
            android:theme="@style/Theme.KPT.NoActionBar"/>

        <activity
            android:name=".Graph"
            android:label="Graph Display"
            android:theme="@style/Theme.KPT.NoActionBar">

        </activity>

    </application>

</manifest>